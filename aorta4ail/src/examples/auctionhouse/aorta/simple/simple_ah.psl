# standard properties
reg_sally: <>(B(sally, registered(sally)))
reg_carol: <>(B(carol, registered(carol)))
ver_sally: <>(B(sally, verified(sally)))
ver_carol: <>(B(carol, verified(carol)))
auction: <>(B(sally, auction(1,lamp,sally,10,0)))
reg: <>(B(sally, registered(sally)) & B(carol, registered(carol)))
ver: <>(B(sally, verified(sally)) & B(carol, verified(carol)))
manager: <>(B(sally, manager(mike)) & B(carol, manager(mike))) 
all: <>(B(sally, registered(sally)) & B(carol, registered(carol)) & B(sally, verified(sally)) & B(carol, verified(carol)) & B(sally, manager(mike)) & B(carol, manager(mike)) & B(sally, auction(1,lamp,sally,10,0)))

# AORTA properties
enactment: <>(Org(sally, rea(sally, customer)) & Org(sally, rea(sally, seller)) & Org(carol, rea(carol, customer)) & Org(mike, rea(mike, manager)))
mike_informed_enactment: <>(Org(carol, rea(mike, manager)) & Org(sally, rea(mike, manager)))
opt_leadsto_goal: <>(Opt(sally, verified(sally)) U Goal(mike, verified(sally)))
commitment: <>(Opt(sally, registered(sally)) U Goal(sally, registered(sally)))
norm_activation: <>(~B(sally, registered(sally)) U Org(sally, norm(sally, buyer, verified(sally), auction_created(sally, lamp))))
norm_fulfillment: <>((~B(sally, registered(sally)) U Org(sally, norm(sally, buyer, verified(sally), auction_created(sally, lamp)))) U (B(sally, verified(sally)) & ~Org(sally, norm(sally, buyer, verified(sally), auction_created(sally, lamp)))))
violation: <>(Org(carol, viol(carol, buyer, paidFor(lamp), ~participates(carol, lamp))) U B(carol, paidFor(lamp)))